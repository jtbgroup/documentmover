<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import be.vds.documentmover.ui.MainPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TabPane?>
<?import be.vds.documentmover.ui.FileTreeView?>
<?import be.vds.documentmover.ui.ActionPane?>

<StackPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0"
	xmlns:fx="http://javafx.com/fxml" fx:controller="be.vds.documentmover.controllers.MoverAppView">
	<VBox>
		<MenuBar>
			<Menu text="File">
				<MenuItem text="Preferences" />
			</Menu>
		</MenuBar>
		<SplitPane orientation="HORIZONTAL">
			<VBox>
				<FileTreeView fx:id="srcTreeView" />
				<FileTreeView fx:id="destTreeView" />
			</VBox>
			<TabPane fx:id="tabPane" />
		</SplitPane>

		<fx:include fx:id="actionPane" source="DocMoverView.fxml" />
	</VBox>

</StackPane>